# Example Environment Configuration
# Copy this file to .env.development and fill in your values

# PostgreSQL Configuration
# The database username
POSTGRES_USER=your_db_user

# The database password (use a strong password in production!)
POSTGRES_PASSWORD=your_secure_password_here

# The database name
POSTGRES_DB=your_database_name

# Database Connection URL
# Format: postgresql://user:password@host:port/database
# Note: Uses custom port 5434 (not default 5432)
DATABASE_URL=postgresql://your_db_user:your_secure_password_here@localhost:5434/your_database_name

# Environment setting
NODE_ENV=development
